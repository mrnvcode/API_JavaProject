<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="rootPane" prefHeight="443.0" prefWidth="515.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller">
    <TextField fx:id="searchField" layoutX="25.0" layoutY="39.0" promptText="Enter movie name" />
    <Button layoutX="186.0" layoutY="39.0" onAction="#searchButtonClicked" text="Search" />
    <ListView fx:id="movieListView" layoutX="253.0" layoutY="39.0" onMouseClicked="#movieSelected" prefHeight="228.0" prefWidth="248.0" />
    <ImageView fx:id="posterImageView" fitHeight="244.0" fitWidth="233.0" layoutX="14.0" layoutY="145.0" preserveRatio="true" />
</AnchorPane>
